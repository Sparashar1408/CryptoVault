{
  "manifest_version": 2,
  "name": "CryptoVault",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["js/jquery-3.5.0.min.js", "js/jquery.validate.min.js", "js/popper.min.js", "js/bootstrap.min.js", "js/sha256.js", "js/aes.js", "js/popup.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
